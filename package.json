{
  "name": "ekko-realtime-cli",
  "description": "A command-line tool for ekko, the realtime serverless platform.",
  "version": "0.0.10",
  "author": "ekko",
  "bin": {
    "ekko": "./bin/run"
  },
  "bugs": "https://github.com/ekko-live/ekko-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "aws-sdk": "^2.894.0",
    "cli-ux": "^5.5.1",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "ora": "^5.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/ekko-live/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "ekko",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ekko-live/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
